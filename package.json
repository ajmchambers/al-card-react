{
  "name": "@ajmchambers/al-card-react",
  "sideEffects": false,
  "version": "0.0.3",
  "description": "React specific wrapper for component-library",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionic.git"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
